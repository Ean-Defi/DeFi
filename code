<script>
    //初始化变量
    max = 10, page = 1;
    $(function () {
        //切换tab标签
        $(".weui-navbar__item").click(function () {
            $(".weui-loadmore").html('<i class="weui-loading"></i> <span class="weui-loadmore__tips">loading data</span>')
            $(".infinite").infinite();
            max = 10, page = 1;
            var findbox = $($(this).attr("href")).find(".content-padded");
            findbox.empty();
            //ajaxdata(page, findbox);
        })

        $(".apply-record .tab .tab-item").click(function () {
            $(this).addClass("active").siblings().removeClass("active");
            $(".products .mainCont").eq($(this).index()).show().siblings().hide();
        })

        $(".apply-record2 .tabs .tab-item2").click(function () {
            $(this).addClass("active").siblings().removeClass("active");
            $(".products2 .mainCont2").eq($(this).index()).show().siblings().hide();
        })
        $(".apply-record3 .tabs3 .tab-item3").click(function () {
            $(this).addClass("active").siblings().removeClass("active");
            $(".products3 .mainCont3").eq($(this).index()).show().siblings().hide();
        })
        var ass = $(this).find("#picker1").val();
        $(".asset").html(ass)
    })
    
    function getData(ele) {
        if("jc"==ele.id){
            var params = {};
            params.type = 1;
            $.ajax({
                url:"/easy/Main/orderlist",
                type: "post",
                dataType: "json",
                data:params,
                success:function(res){
                    var res = JSON.parse(res);
                    res = res.data;
                    var findbox = $("#jcdiv").find(".content-padded");
                    for (var i = 0; i < res.data.length; i++) {
                        $("#jcdiv").find(".content-padded").append('<ul class="tab3-ul">' +
                        '<li class="li-one" style="whith">' +
                        '<p>' + res.data[i].coin + '</p>' +
                        '<P>' + res.data[i].order_time + '</P>' +
                        '</li>' +
                        '<li>' +
                        '<p>Duration</p>' +
                        '<p>' + res.data[i].contract_period + '</p>' +
                        '</li>' +
                        '<li>' +
                        '<p>Quantity</p>' +
                        '<p>' + res.data[i].quantity + '</p>' +
                        '</li>' +
                        '<li>' +
                        '<p>Value(USDT)</p>' +
                        '<p>' + res.data[i].usdt_quantity + '</p>' +
                        '</li>' +
                        '<li>' +
                        '<p>interest rate</p>' +
                        '<p>' + res.data[i].rate + '</p>' +
                        '</li>' +
                        '<li>' +
                        '<p>Status</p>' +
                        '<p>' + res.data[i].status + '</p>' +
                        '</li>' +
                        '<li>' +
                        '<p>Expiry</p>' +
                        '<p>' + res.data[i].expiration_time + '</p>' +
                        '</li>' +
                        '</ul>')
                    }
                }
            });
        }else{
            var params = {};
            params.type = 2;
            $.ajax({
                url:"/easy/Main/orderlist",
                type: "post",
                dataType: "json",
                data:params,
                success:function(res){
                    var res = JSON.parse(res);
                    res = res.data;
                    var findbox = $("#jrdiv").find(".content-padded");
                    for (var i = 0; i < res.data.length; i++) {
                        var status_tit = '';
                        var status_val = '';
                        if (res.data[i].status == "Open") {
                            status_tit = "Operation";
                            status_val = "repayment";
                        } else {
                            status_tit = "";
                            status_val = "";
                        }
                        $("#jrdiv").find(".content-padded").append('<ul class="tab3-ul">' +
                            '<li class="li-one">' +
                        '<p>' + res.data[i].coin + '</p>' +
                        '<P>' + res.data[i].order_time + '</P>' +
                        '</li>' +
                        '<li>' +
                        '<p>Duration</p>' +
                        '<p>' + res.data[i].contract_period + '</p>' +
                        '</li>' +
                        '<li>' +
                        '<p>Quantity</p>' +
                        '<p>' + res.data[i].quantity + '</p>' +
                        '</li>' +
                        '<li>' +
                        '<p>Value(USDT)</p>' +
                        '<p>' + res.data[i].usdt_quantity + '</p>' +
                        '</li>' +
                        '<li>' +
                        '<p>interest rate</p>' +
                        '<p>' + res.data[i].rate + '</p>' +
                        '</li>' +
                        '<li>' +
                        '<p>Status</p>' +
                        '<p>' + res.data[i].status + '</p>' +
                        '</li>' +
                        '<li>' +
                        '<p>Expected Interest</p>' +
                        '<p>' + res.data[i].total_interest + '</p>' +
                        '</li>' +
                        '<li>' +
                        '<p>Expiry</p>' +
                        '<p>' + res.data[i].expiration_time + '</p>' +
                        '</li>' +
                        '<li>' +
                        '<p class="order-action">' + status_tit + '</p>' +
                        '<p class="action">' +
                        '<a href="javascript:;" data-id="' + res.data[i].orderId + '" class="">' + status_val + '</a>' +
                        '</p>' +
                        '</li>' +
                        '</ul>')
                    }
                }
            });
        }
    }
    //ajax加载数据 p为page ele为元素
    function ajaxdata(p, ele) {
        //判断不同的tab标签
        if (ele.parents('.weui-tab__bd-item').attr("id") == "tab3") {
            var url = "/easy/Main/orderlist";
            var params = {};
            params.type = 2;
            params.coin = coin_o;
            params.contract_period = contract_period_o;
            params.status = status_o;
            params.page = page;
            $.post(url, params, function (res) {
                var res = JSON.parse(res);
                res = res.data;
                if (res.data.length == 0 || res.data.length < res.per_page) {
                    //没有数据时
                    $(".tab3").css('height', 'auto');
                    $(".infinite").destroyInfinite()
                    $(".weui-loadmore").html('<div class="weui-loadmore weui-loadmore_line"> <span class="weui-loadmore__tips">No information</span> </div>')
                }
                for (var i = 0; i < res.data.length; i++) {
                    ele.append('<ul class="tab3-ul">' +
                        '<li class="li-one">' +
                        '<p>' + res.data[i].coin + '</p>' +
                        '<P>' + res.data[i].order_time + '</P>' +
                        '</li>' +
                        '<li>' +
                        '<p>Duration</p>' +
                        '<p>' + res.data[i].contract_period + '</p>' +
                        '</li>' +
                        '<li>' +
                        '<p>Quantity</p>' +
                        '<p>' + res.data[i].quantity + '</p>' +
                        '</li>' +
                        '<li>' +
                        '<p>Value(USDT)</p>' +
                        '<p>' + res.data[i].usdt_quantity + '</p>' +
                        '</li>' +
                        '<li>' +
                        '<p>Status</p>' +
                        '<p>' + res.data[i].status + '</p>' +
                        '</li>' +
                        '<li>' +
                        '<p>Entry</p>' +
                        '<p>' + res.data[i].expiration_time + '</p>' +
                        '</li>' +
                
                        '</ul>')
                }
            })
        } else if (ele.parents('.weui-tab__bd-item').attr("id") == "tab4") {
            var url = "/easy/Main/orderlist";
            var params = {};
            params.type = 2;
            params.coin = coin_o;
            params.contract_period = contract_period_o;
            params.status = status_o;
            params.page = page;
            $.post(url, params, function (res) {
                var res = JSON.parse(res);
                res = res.data;
                if (res.data.length == 0 || res.data.length < res.per_page) {
                    $(".infinite").destroyInfinite();
                    $(".weui-loadmore").html('<div class="weui-loadmore weui-loadmore_line"> <span class="weui-loadmore__tips">No information</span> </div>')
                }
                for (var i = 0; i < res.data.length; i++) {
                    var status_tit = '';
                    var status_val = '';
                    if (res.data[i].status == "Open") {
                        status_tit = "Operation";
                        status_val = "repayment";
                    } else {
                        status_tit = "";
                        status_val = "";
                    }
                    ele.append('<ul class="tab3-ul">' +
                        '<li class="li-one">' +
                        '<p>' + res.data[i].coin + '</p>' +
                        '<P>' + res.data[i].order_time + '</P>' +
                        '</li>' +
                        '<li>' +
                        '<p>Duration</p>' +
                        '<p>' + res.data[i].contract_period + '</p>' +
                        '</li>' +
                        '<li>' +
                        '<p>Quantity</p>' +
                        '<p>' + res.data[i].quantity + '</p>' +
                        '</li>' +
                        '<li>' +
                        '<p>Value(USDT)</p>' +
                        '<p>' + res.data[i].usdt_quantity + '</p>' +
                        '</li>' +
                        '<li>' +
                        '<p>Status</p>' +
                        '<p>' + res.data[i].status + '</p>' +
                        '</li>' +
                        '<li>' +
                        '<p>Expected Interest</p>' +
                        '<p>' + res.data[i].total_interest + '</p>' +
                        '</li>' +
                        '<li>' +
                        '<p>Expiry</p>' +
                        '<p>' + res.data[i].expiration_time + '</p>' +
                        '</li>' +
                        '<li>' +
                        '<p class="order-action">' + status_tit + '</p>' +
                        '<p class="action">' +
                        '<a href="javascript:;" data-id="' + res.data[i].orderId + '" class="">' + status_val + '</a>' +
                        '</p>' +
                        '</li>' +
                        '</ul>')
                }
            })
        } else if (ele.parents('.weui-tab__bd-item').attr("id") == "tab5") {
            var url = "/easy/Main/backlist";
            var params = {};
            params.startime = '';
            params.endtime = '';
            params.page = page;
            $.post(url, params, function (res) {
                var res = JSON.parse(res);
                res = res.data;
                if (res.data.length == 0 || res.data.length < res.per_page) {
                    $(".infinite").destroyInfinite()
                    $(".weui-loadmore").html('<div class="weui-loadmore weui-loadmore_line"> <span class="weui-loadmore__tips">No information</span> </div>')
                }
                for (var i = 0; i < res.data.length; i++) {
                    ele.append('<ul class="tab3-ul">' +
                        '<li class="li-one">' +
                        '<p>Trader</p>' +
                        '<p>' + res.data[i].userName + '</p>' +
                        '</li>' +
                        '<li class="li-one">' +
                        '<p>Account Classification</p>' +
                        '<p>' + res.data[i].level + '</p>' +
                        '</li>' +
                        '<li class="li-one">' +
                        '<p>Asset</p>' +
                        '<p>' + res.data[i].coin + '</p>' +
                        '</li>' +
                        '<li class="li-one">' +
                        '<p>Income Earned</p>' +
                        '<P>' + res.data[i].deal_fee + '</P>' +
                        '</li>' +
                        '<li class="li-one">' +
                        '<p>Income Type</p>' +
                        '<P>' + res.data[i].fx + '</P>' +
                        '</li>' +
                        '<li class="li-one">' +
                        '<p>Date/Time</p>' +
                        '<P>' + res.data[i].addtime + '</P>' +
                        '</li>' +
                        '</ul>')
                }
            })
        }
    }
