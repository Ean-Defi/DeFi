    $('#ya_c').keyup(function () {
        yc_c = $(this).val() * 1;
        var coin = $("#picker1").val();
        time = $('.days option:selected').attr('data-time')
        $.ajax({
            url: "/easy/Main/dEanInterest", //请求接口地址
            type: 'post', //请求方式
            dataType: 'json', //数据类型
            data: {
                'num': yc_c,
                'type': '1',
                'time': time,
                'coin': coin
            },
            success: function (res) {
                var res = JSON.parse(res);
                $("#cdean").val(res.res);
            }
        });
        time = $(".days option:selected").attr('data-time')
        $('#yqsy').val(((day_fee_c * time * yc_c) / 100).toFixed(8));
        $('#usdt_num').val((newprice_c*yc_c));

    });
    // 抵押的数量
    $('#ya_j').keyup(function () {
        yc_j = $(this).val() * 1;
        $('.cny-span').html((newprice_j * yc_j).toFixed(8));
        $('#get_num').val((newprice_j*yc_j*0.7).toFixed(8));
        $('#usdt_num').val((newprice_j*yc_c*0.7).toFixed(8));
        $('#get_num3').val((newprice_j*yc_j*0.7).toFixed(8));
        time=$(".days2 option:selected").attr('data-time')
        $('#getNum_fee').val(((time * 1 * yc_j * 0.7) / 100).toFixed(8));
        var ya_j = $('#get_num').val();
        $.ajax({
            url: "/easy/Main/dEanInterest", //请求接口地址
            type: 'post', //请求方式
            dataType: 'json', //数据类型
            data: {
                'num': ya_j,
                'type': '2',
                'time': time,
                'coin': 'USDT',
            },
