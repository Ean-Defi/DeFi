<script type="text/javascript" src="/templates/wap/view/default/easy/index.js?v=1609342829"></script>
<script type="text/javascript">
    var cCoin = ["USDT","BTC","GOLDMOON","ETH","BTCMOON","ETHMOON","TRX"];
    var jCoin = ["USDT","BTC","ETH","GOLDMOON","BTCMOON","ETHMOON","TRX"];
    // var cCoin = ["USDT","BTC","GOLDMOON","ETH","BTCMOON","ETHMOON","TRX"];
    // var jCoin = ["USDT","BTC","ETH","GOLDMOON","BTCMOON","ETHMOON","TRX"];
    $(function () {
        $.toast.prototype.defaults.duration = 1000;//设置弹框显示时间为1秒
        $('.mode').on('click', 'li', function () {
            $(this).addClass('on').siblings().removeClass('on');
        })
        $('#yearslilv').val(seven_c)
        $('#year_free_j').val(seven_j)
    });

    // 存入的数量
    var yc_c=$('#ya_c').val()*1;
    // 抵押的数量
    var yc_j=$('#ya_j').val()*1;

    $(".days").change(function(){
        fee_id = $('.days option:selected').attr('data-id');
        time = $('.days option:selected').attr('data-time');
        dean_c();
        if(fee_id==1){
            $('#yearslilv').val(seven_c);
            $('.day_fee_c').html((seven_c / 365).toFixed(8)+'%');
            day_fee_c = (seven_c / 365);
            $('#yqsy').val(((day_fee_c*time*yc_c)/100).toFixed(8));
            $('#usdt_num').val((newprice_c*yc_c))
        }else if(fee_id==2){
            $('#yearslilv').val(fourteen_c);
            $('.day_fee_c').html((fourteen_c / 365).toFixed(8)+'%');
            day_fee_c = (fourteen_c / 365);
            $('#yqsy').val(((day_fee_c*time*yc_c)/100).toFixed(8));
            $('#usdt_num').val((newprice_c*yc_c))
        }else if(fee_id==3){
            $('#yearslilv').val(thirty_c);
            $('.day_fee_c').html((thirty_c / 365).toFixed(8)+'%');
            day_fee_c = (thirty_c / 365);
            $('#yqsy').val(((day_fee_c*time*yc_c)/100).toFixed(8));
            $('#usdt_num').val((newprice_c*yc_c))
        }else if(fee_id==4){
            $('#yearslilv').val(ninety_c);
            $('.day_fee_c').html((ninety_c / 365).toFixed(8)+'%');
            day_fee_c = (ninety_c / 365);
            $('#yqsy').val(((day_fee_c*time*yc_c)/100).toFixed(8));
            $('#usdt_num').val((newprice_c*yc_c))
        }
    });

    $(".days2").change(function(){
        fee_id = $('.days2 option:selected').attr('data-id');
