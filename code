    $('.mode1').on('click', 'li', function () {
        fee_id = $(this).children().attr('data-id');
        time = $(this).children().attr('data-time');
        dean_c();
        if (fee_id == 1) {
            $('#year_fee_c').html(seven_c + '%');
            $('.day_fee_c').html((seven_c / 365).toFixed(8) + '%');
            day_fee_c = (seven_c / 365);
            $('#yqsy').val(((day_fee_c * time * yc_c) / 100).toFixed(8));
        } else if (fee_id == 2) {
            $('#year_fee_c').html(fourteen_c + '%');
            $('.day_fee_c').html((fourteen_c / 365).toFixed(8) + '%');
            day_fee_c = (fourteen_c / 365);
