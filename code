    function dean_c() {
        var ya_c = $('#ya_c').val() * 1;
        var coin = $("#picker1").val();
        $.ajax({
            url: "/easy/Main/dEanInterest", //请求接口地址
            type: 'post', //请求方式
            dataType:'json', //数据类型
            data: {
                'num': ya_c,
                'type': '1',
                'time': 7,
                'coin': coin,
            },
            success: function (res) {
                var res = JSON.parse(res);
                $("#cdean").val(res.res);
            },
        });
    }
    function dean_j() {
        var ya_j = $('#get_num').val();
        $.ajax({
            url: "/easy/Main/dEanInterest", //请求接口地址
            type: 'post', //请求方式
            dataType: 'json', //数据类型
            data: {
                'num': ya_j,
                'type': '2',
                'time': 7,
                'coin': 'USDT',
            },
            success: function (res) {

                var res = JSON.parse(res);
                $("#jdean").val(res.res);
            },
        });
    }
