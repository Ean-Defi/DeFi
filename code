    // 抵押的数量
    var yc_j = $('#ya_j').val() * 1;
    // 获取借入利率
    $('.ya_borrow').click(function () {
        $.ajax({
            url: "/easy/Main/getcoincoinf", //请求接口地址
            type: 'post', //请求方式
            dataType: 'json', //数据类型
            data: {
                'coin': usdt_j,
                'type': '2'
            },
            success: function (res) {
                var res = JSON.parse(res);
                newprice_j = res.data.newprice;
                seven_j = res.data.seven;
                fourteen_j = res.data.fourteen;
                thirty_j = res.data.thirty;
                ninety_j = res.data.ninety;
                day_fee_j = (seven_j / 365);
                $('#year_fee_j').val(seven_j);
                // $('#year_fee_j').html(seven_j + '%');
                $('.day_fee_j').html((seven_j / 365).toFixed(8) + '%');
                $('.cny-span').html((newprice_j * yc_j).toFixed(8));
                $('#get_num').val((newprice_j * yc_j * 0.7).toFixed(8));
                $('#getNum_fee').val(((time * newprice_j * yc_j * 0.7) / 100).toFixed(8));

            },
        });
    })
    // 借出切换币种
    $('#picker1').change(function () {
        var usdt_c = $("#picker1").val();
        $.ajax({
            url: "/easy/Main/getcoincoinf",
            async: true,
            dataType: 'json',
            type: "post",
            data: {
                'coin': usdt_c,
                'type': '1'
            },
            success: function (res) {
                var res = JSON.parse(res);
                newprice_c = res.data.newprice;
                seven_c = res.data.seven;
                fourteen_c = res.data.fourteen;
                thirty_c = res.data.thirty;
                ninety_c = res.data.ninety;
                day_fee_c = (seven_c / 365);
                $('#year_fee_c').html(seven_c + '%');
                $('#yearslilv').val(seven_c + '%')
                $('.day_fee_c').html((seven_c / 365).toFixed(8) + '%');
                $('#yqsy').val(((day_fee_c * time * yc_c) / 100).toFixed(8));
                time=$(".days option:selected").attr('data-time')
                $("#expectedReturn").val(((day_fee_c * time * yc_c) / 100).toFixed(8));
                $('#usdt_num').val((newprice_c*yc_c))
                dean_c();
            }
        });
    });
    // 借入切换币种
