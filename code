<script type="text/javascript">
    $(function(){
        var u = navigator.userAgent;
        var isAndroid = u.indexOf('Android') > -1 || u.indexOf('Adr') > -1;
        if(isAndroid){
            $('input[type="text"],input[type="number"]').bind('focus',function(){
                $('nav .weui-tabbar').css({'position':'static'});
                $('.login-bot').css({'position':'static'});
            }).bind('blur',function(){
                $('nav .weui-tabbar').css({'position':'fixed','bottom':'0'});
                $('.login-bot').css({'position':'fixed','bottom':'0'});
            });
        }
        if($('.weui-tabbar__item.weui-bar__item--on')){
            if($('.weui-tabbar__item.weui-bar__item--on').find(".weui-tabbar__icon").children().attr('src') == '/static/img/Union2.png'){
                $('.weui-tabbar__item.weui-bar__item--on').find(".weui-tabbar__icon").children().attr('src','/static/img/Union.png');
            }
            if($('.weui-tabbar__item.weui-bar__item--on').find(".weui-tabbar__icon").children().attr('src') == '/static/img/deal2.png'){
                $('.weui-tabbar__item.weui-bar__item--on').find(".weui-tabbar__icon").children().attr('src','/static/img/deal.png');
            }
            if($('.weui-tabbar__item.weui-bar__item--on').find(".weui-tabbar__icon").children().attr('src') == '/static/img/bazaar2.png'){
                $('.weui-tabbar__item.weui-bar__item--on').find(".weui-tabbar__icon").children().attr('src','/static/img/bazaar.png');
            }
            if($('.weui-tabbar__item.weui-bar__item--on').find(".weui-tabbar__icon").children().attr('src') == '/static/img/wallet2.png'){
                $('.weui-tabbar__item.weui-bar__item--on').find(".weui-tabbar__icon").children().attr('src','/static/img/wallet.png');
            }
            if($('.weui-tabbar__item.weui-bar__item--on').find(".weui-tabbar__icon").children().attr('src') == '/static/img/more2.png'){
                $('.weui-tabbar__item.weui-bar__item--on').find(".weui-tabbar__icon").children().attr('src','/static/img/more.png');
            }
        }
        //console.dir($('.weui-tabbar__item.weui-bar__item--on').find(".weui-tabbar__icon").children().attr('src'));
    });
</script>
